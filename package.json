{
  "name": "docupress",
  "version": "1.0.0",
  "description": "一款SEO友好的博客、文档站点生成器",
  "keywords": [
    "Docu",
    "Press",
    "DocuPress"
  ],
  "license": "MIT",
  "author": "陈佳宝，mail@chenjiabao.com",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mojiexuan/docupress.git"
  },
  "homepage": "https://doc.chenjiabao.com",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "bin": {
      "docupress": "./dist/esm/cli.js"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pnpm build"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "pnpm@10.27.0",
  "dependencies": {
    "@mdit/plugin-alert": "^0.21.0",
    "@mdit/plugin-figure": "^0.21.0",
    "@mdit/plugin-img-lazyload": "^0.21.0",
    "@mdit/plugin-img-size": "^0.21.0",
    "@mdit/plugin-katex": "^0.21.0",
    "@mdit/plugin-plantuml": "^0.21.0",
    "@mdit/plugin-ruby": "^0.21.0",
    "@mdit/plugin-spoiler": "^0.21.0",
    "@mdit/plugin-tasklist": "^0.21.0",
    "@shikijs/transformers": "^3.5.0",
    "@types/echarts": "^5.0.0",
    "echarts": "^5.6.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "json5": "^2.2.3",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-async": "^2.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-container": "^4.0.0",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-ins": "^4.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "nunjucks": "^3.2.4",
    "shiki": "^3.5.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-attrs": "^4.1.3",
    "@types/node": "^22.15.16",
    "@types/nunjucks": "^3.2.6",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup": "^4.42.0",
    "rollup-plugin-copy": "^3.5.0"
  }
}
